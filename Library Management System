import java.util.Scanner;

class Book {
	int book_id;
	String title;
	String author;
	String publisher;
	int year_of_publishing;
	boolean available = true;
	boolean activated = true;
	String description;
	
	Book(int book_id, String title, String author, String publisher, int year_of_publishing, String description) {
		this.book_id = book_id;
		this.title = title;
		this.author = author;
		this.publisher = publisher;
		this.year_of_publishing = year_of_publishing;
		this.description = description;
	}
		
}

class Member {
	int member_id;
	String member_name;
	String lib_card_num;
	String password;
	boolean ative_status = true;
	
	Member(int member_id, String member_name, String lib_card_num, String password) {
		this.member_id = member_id;
		this.member_name = member_name;
		this.lib_card_num = lib_card_num;
		this.password = password;
	}
}

class Author {
	int author_id;
	String author_name;
	String country;
	String [] books_written;
	
	Author(int author_id, String author_name, String country) {
		this.author_id = author_id;
		this.author_name = author_name;
		this.country = country;
	}
}
class Admin {
	int admin_id;
	String admin_name;
	String position;
	String password;
	String email;
	
	Admin(int admin_id, String admin_name, String position, String password, String email){
		this.admin_id = admin_id;
		this.position = position;
		this.password = password;
		this.email = email;
	}
}
